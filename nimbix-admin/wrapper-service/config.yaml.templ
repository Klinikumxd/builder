image: t1
instance_type: ngd3
max_time_minutes: 40
shared_secret: CHANGETHIS
script: |
  cd /tmp
  rm -Rf /tmp/pytorch-builder
  sudo apt-get install -y git
  git clone git@github.com:pytorch/builder.git pytorch-builder --quiet
  cd pytorch-builder
  bash nimbix-bootstrap.sh {project} {commit_hash} {branch}
