image: t1
instance_type: ngd3
max_time_minutes: 40
shared_secret: CHANGETHIS
script: |
  cd /tmp
  rm -Rf /tmp/pytorch-builder
  sudo apt-get install -y git
  git clone https://pytorchbot:{ghtoken}@github.com/pytorch/builder pytorch-builder --quiet
  cd pytorch-builder
  bash nimbix-bootstrap.sh {project} {commit_hash} {branch} {ghtoken}
